#~ msgstr "Manum Lentam vincere"

#~ msgid "Death of Kapou'e"
#~ msgstr "Mors Kapouis"

#~ msgid "Death of Grüü"
#~ msgstr "Mors Gruus"

#~ msgid "Transport"
#~ msgstr "Oneraria"

